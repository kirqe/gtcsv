{
  "name": "GTCSV",
  "description": "GTCSV saves your word decks to csv files",
  "icons": {
    "128": "gtcsv-app-icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://translate.google.com/*"],
      "js": ["contentscript.js"],
      "css": ["contentscript.css"]
    }
  ],
  "version": "0.0.1",
  "manifest_version": 2
}
